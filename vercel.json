{
    "buildCommand": "",
    "cleanUrls": true,
    "devCommand": "",
    "framework": null,
    "functions": {
        "api/build/main.js": {
            "includeFiles": "api/build/**"
        }
    },
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ],
    "installCommand": "yarn install --frozen-lockfile",
    "outputDirectory": "app/build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/build/main"
        }
    ],
    "trailingSlash": false
}