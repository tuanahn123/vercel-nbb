{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/projects/nbb-clients/assets/:filePath",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/projects/nbb-clients"
        }
    ]
}